{
  "babelrc": false,
  "comments": false,
  "plugins": [
    "babel-plugin-transform-export-extensions",
    [
      "babel-plugin-transform-dev",
      {
        "evaluate": false
      }
    ],
    [
      "babel-plugin-transform-runtime",
      {
        "helpers": true,
        "polyfill": true,
        "regenerator": false
      }
    ]
  ],
  "presets": [
    [
      "babel-preset-env",
      {
        "modules": false,
        "shippedProposals": true,
        "targets": {
          "ie": 10
        },
        "useBuiltIns": "usage"
      }
    ],
    "babel-preset-stage-2",
    "minify"
  ]
}