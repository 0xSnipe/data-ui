{
  "name": "@data-ui/data-table",
  "version": "0.0.25",
  "description": "Table components that can handle a lot of data",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack -p && npm run copy-styles",
    "copy-styles": "cp ./node_modules/react-virtualized/styles.css ./build/styles.css",
    "dev": "webpack --progress --colors --watch",
    "dev:nowatch": "webpack -d --progress --colors",
    "prepublish": "npm run build",
    "test": "jest --colors --verbose --coverage"
  },
  "repository": "https://github.com/williaster/data-ui",
  "keywords": [
    "data",
    "table",
    "react",
    "react-virtualized"
  ],
  "author": "Chris Williams <chris.williams@airbnb.com>",
  "license": "MIT",
  "homepage": "https://github.com/williaster/data-ui#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "prop-types": "^15.5.10",
    "react-virtualized": "^9.7.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-airbnb": "^2.2.3",
    "css-loader": "^0.28.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "jest": "^20.0.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "webpack": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ]
  }
}
