{
  "name": "@data-ui/sparkline",
  "version": "0.0.59",
  "description": "React + d3 library for creating sparklines",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "esm/index.js",
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "build:cjs": "beemo babel ./src --out-dir lib/",
    "build:esm": "beemo babel ./src --esm --out-dir esm/",
    "build": "npm run build:cjs && npm run build:esm",
    "jest": "beemo jest --verbose --coverage --color",
    "lint": "beemo eslint .",
    "posttest": "npm run lint",
    "test": "npm run jest",
    "prettier": "beemo prettier \"./{src,tests}/**/*.{js,jsx,json,md}\" --write",
    "sync:gitignore": "beemo sync-dotfiles --filter=gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/williaster/data-ui.git"
  },
  "keywords": [
    "vx",
    "react",
    "d3",
    "visualization",
    "sparkline",
    "graph",
    "chart",
    "data"
  ],
  "author": "Chris Williams @williaster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/williaster/data-ui/issues"
  },
  "homepage": "https://github.com/williaster/data-ui#readme",
  "devDependencies": {
    "@beemo/cli": "^0.19.0",
    "@beemo/core": "^0.19.2",
    "@data-ui/build-config": "^0.0.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0",
    "react-dom": "^15.0.0-0 || ^16.0.0-0"
  },
  "dependencies": {
    "@data-ui/shared": "0.0.59",
    "@data-ui/theme": "0.0.8",
    "@vx/axis": "0.0.140",
    "@vx/curve": "0.0.140",
    "@vx/event": "0.0.143",
    "@vx/glyph": "0.0.140",
    "@vx/gradient": "0.0.140",
    "@vx/group": "0.0.140",
    "@vx/pattern": "0.0.140",
    "@vx/point": "0.0.136",
    "@vx/responsive": "0.0.140",
    "@vx/scale": "0.0.140",
    "@vx/shape": "0.0.140",
    "d3-array": "^1.2.0",
    "prop-types": "^15.5.10"
  },
  "beemo": {
    "module": "@data-ui/build-config",
    "drivers": [
      "prettier",
      "eslint"
    ]
  }
}
